Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"CG", "(", 
     RowBox[{"\[Lambda]", ",", "\[Gamma]"}], ")"}], ":=", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Product]", 
       RowBox[{"j", "=", "1"}], 
       RowBox[{"d", "-", "1"}]], 
      RowBox[{"(", GridBox[{
         {
          SubscriptBox["\[Gamma]", "j"], 
          SubscriptBox["\[Lambda]", 
           RowBox[{"j", "+", "1"}]], 
          SubscriptBox["\[Gamma]", 
           RowBox[{"j", "+", "1"}]]},
         {
          SubscriptBox["s", "j"], 
          SubscriptBox["m", 
           RowBox[{"j", "+", "1"}]], 
          SubscriptBox["s", 
           RowBox[{"j", "+", "1"}]]}
        }], ")"}]}], "\[Element]", 
     RowBox[{
      SuperscriptBox[
       TemplateBox[{},
        "Reals"], 
       RowBox[{
        RowBox[{"2", 
         SubscriptBox["\[Lambda]", "1"]}], "+", "1"}]], "\[CircleTimes]", 
      "\[Ellipsis]", "\[CircleTimes]", 
      SuperscriptBox[
       TemplateBox[{},
        "Reals"], 
       RowBox[{
        RowBox[{"2", 
         SubscriptBox["\[Lambda]", "d"]}], "+", "1"}]], "\[CircleTimes]", 
      SuperscriptBox[
       TemplateBox[{},
        "Reals"], 
       RowBox[{
        RowBox[{"2", 
         SubscriptBox["\[Gamma]", "d"]}], "+", "1"}]]}]}]}], ","}], 
  TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.963076926654972*^9, 3.9630769619863453`*^9}, {
   3.9630770753080597`*^9, 3.963077080156849*^9}, {3.9630886760630875`*^9, 
