 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Hom", "G"], "(", 
    RowBox[{
     SubscriptBox["H", 
      FormBox["\[Rho]",
       TraditionalForm]], ",", 
     RowBox[{
      SuperscriptBox["\[ScriptCapitalS]", "D"], "(", 
      SuperscriptBox["X", "*"], ")"}]}], ")"}], TraditionalForm]],
